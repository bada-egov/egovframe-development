package egovframework.dev.imp.commngt.wizards.model;

import java.util.List;

import org.eclipse.jdt.core.IJavaProject;
/**
 * 공통컴포넌트 생성 컨텍스트 클래스
 * @author 개발환경 개발팀 박수림
 * @since 2011.06.13
 * @version 1.0
 * @see <pre>
 * &lt;&lt; 개정이력(Modification Information) &gt;&gt;
 *   
 *   수정일      수정자           수정내용
 *  -------    --------    ---------------------------
 *   2011.06.13  박수림          최초 생성
 * 
 * 
 * </pre>
 */
public class NewEgovCommngtContext {
	/** 설치할 컴포넌트 */
	private List<IComponentElement> component;
	/** 인스톨타입 */
	private String installType = "";
	/** Datasource - DB 타입 */
	private String dBType = "";
	/** Datasource - url */
	private String url = "";
	/** Datasource - username */
	private String username = "";
	/** Datasource - password */
	private String password = "";
	/** DB Connection Test 성공여부 */
	private boolean connectionTest = false;
	/** 선택프로젝트 */
	private IJavaProject javaProject;
	/** web.xml 수정여부(사용자가 선택한 정보) */
	private boolean webModify = false;
	/** 댓글, 만족도, 스크랩 기능 사용여부(globals.properties에 저장할 정보) */
	private boolean addedOptions = false;

	/**
	 * @return the addedOptions
	 */
	public boolean isAddedOptions() {
		return addedOptions;
	}
	/**
	 * setter
	 * @param addedOptions the addedOptions to set
	 */
	public void setAddedOptions(boolean addedOptions) {
		this.addedOptions = addedOptions;
	}
	/**
	 * @return the webModify
	 */
	public boolean isWebModify() {
		return webModify;
	}
	/**
	 * setter
	 * @param webModify the webModify to set
	 */
	public void setWebModify(boolean webModify) {
		this.webModify = webModify;
	}
	/**
	 * @return the component
	 */
	public List<IComponentElement> getComponent() {
		return component;
	}
	/**
	 * setter
	 * @param component the component to set
	 */
	public void setComponent(List<IComponentElement> component) {
		this.component = component;
	}
	/**
	 * @return the installType
	 */
	public String getInstallType() {
		return installType;
	}
	/**
	 * setter
	 * @param installType the installType to set
	 */
	public void setInstallType(String installType) {
		this.installType = installType;
	}
	/**
	 * @return the dBType
	 */
	public String getDBType() {
		return dBType;
	}
	/**
	 * setter
	 * @param dBType the dBType to set
	 */
	public void setDBType(String dBType) {
		this.dBType = dBType;
	}
	/**
	 * @return the url
	 */
	public String getUrl() {
		return url;
	}
	/**
	 * setter
	 * @param url the url to set
	 */
	public void setUrl(String url) {
		this.url = url;
	}
	/**
	 * @return the username
	 */
	public String getUsername() {
		return username;
	}
	/**
	 * setter
	 * @param username the username to set
	 */
	public void setUsername(String username) {
		this.username = username;
	}
	/**
	 * @return the password
	 */
	public String getPassword() {
		return password;
	}
	/**
	 * setter
	 * @param password the password to set
	 */
	public void setPassword(String password) {
		this.password = password;
	}
	/**
	 * @return the connectionTest
	 */
	public boolean isConnectionTest() {
		return connectionTest;
	}
	/**
	 * setter
	 * @param connectionTest the connectionTest to set
	 */
	public void setConnectionTest(boolean connectionTest) {
		this.connectionTest = connectionTest;
	}
	/**
	 * @return the javaProject
	 */
	public IJavaProject getJavaProject() {
		return javaProject;
	}
	/**
	 * setter
	 * @param javaProject the javaProject to set
	 */
	public void setJavaProject(IJavaProject javaProject) {
		this.javaProject = javaProject;
	}
	
}
